    var myChart;
    var myChart2;
    var myChart3;
    var myChart4;
    
    function getVals() {
         $.get( "GetCurrent", function( data ) {
             console.log(data);
             var obj = jQuery.parseJSON(data);
             myChart.data.labels.splice(0,myChart.data.labels.length);
              myChart2.data.labels.splice(0,myChart2.data.labels.length);
               myChart3.data.labels.splice(0,myChart4.data.labels.length);
               myChart4.data.labels.splice(0,myChart4.data.labels.length);
            myChart.data.datasets.forEach((dataset) => {
               dataset.data.splice(0,dataset.data.length);
            });
            myChart2.data.datasets.forEach((dataset) => {
               dataset.data.splice(0,dataset.data.length);
            });
             myChart3.data.datasets.forEach((dataset) => {
               dataset.data.splice(0,dataset.data.length);
            });
             myChart4.data.datasets.forEach((dataset) => {
               dataset.data.splice(0,dataset.data.length);
            });
             $.each(obj, function( i, l ){
                 
                 myChart.data.labels.push(l.Time);
                  myChart2.data.labels.push(l.Time);
                   myChart3.data.labels.push(l.Time);
                    myChart4.data.labels.push(l.Time);
                  myChart.data.datasets[0].data.push((parseFloat(l.Phase1)*220*0.8/1000).toFixed(2));
                  myChart2.data.datasets[0].data.push((parseFloat(l.Phase2)*220*0.8/1000).toFixed(2));
                   myChart3.data.datasets[0].data.push((parseFloat(l.Phase3)*220*0.8/1000).toFixed(2));
                   
                   myChart4.data.datasets[0].data.push(((parseFloat(l.Phase1)+parseFloat(l.Phase2)+parseFloat(l.Phase3))*220*0.8/1000).toFixed(2));
               
              
              
            });
            
//          var vals = data.split(',');
//          
        $( "#val1" ).html((parseFloat(obj[1].Phase1)*220*0.8/1000).toFixed(2) +" Kw");
         $( "#val2" ).html((parseFloat(obj[1].Phase2)*220*0.8/1000).toFixed(2) +" Kw");
          $( "#val3" ).html((parseFloat(obj[1].Phase3)*220*0.8/1000).toFixed(2) +" Kw");
          $( "#total" ).html(((parseFloat(obj[1].Phase1)+parseFloat(obj[0].Phase2)+parseFloat(obj[0].Phase3))*220*0.8/1000).toFixed(2) +" Kw");
          myChart.update();
          myChart2.update();
           myChart3.update();
            myChart4.update();
          
        });
        
        
}


 function initPickColor() {
    $('.pick-class-label').click(function() {
      var new_class = $(this).attr('new-class');
      var old_class = $('#display-buttons').attr('data-class');
      var display_div = $('#display-buttons');
      if (display_div.length) {
        var display_buttons = display_div.find('.btn');
        display_buttons.removeClass(old_class);
        display_buttons.addClass(new_class);
        display_div.attr('data-class', new_class);
      }
    });
  }

